<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<!-- springboot依赖的父工程，使用的 springboot2.0.3.RELEASE版本 -->
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.1.0.RELEASE</version>
		<relativePath /> <!-- lookup parent from repository -->
	</parent>
	<groupId>com.itrus</groupId>
	<artifactId>common-product</artifactId>
	<version>1.5.6</version>
	<name>common-product</name>
	<description>common-product</description>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<configuration>
					<source>${java.version}</source> <!--指明源码用的Jdk版本 -->
					<target>${java.version}</target> <!--指明打包后的Jdk版本 -->
					<encoding>${project.build.sourceEncoding}</encoding>
					<excludes>
						<exclude>application.yml</exclude>
						<exclude>log4j2-spring.xml</exclude>
						<exclude>com/itrus/common/WebApplication.class</exclude>
					</excludes>
				<skip>true</skip>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<executions>
					<execution>
						<id>attach-sources</id>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<skip>true</skip>
				</configuration>
			</plugin>
			<!-- <plugin> <groupId>org.apache.maven.plugins</groupId> <artifactId>maven-surefire-plugin</artifactId> 
				<configuration> 开关单元测试 <skipTests>false</skipTests> <forkMode>never</forkMode> 
				<argLine>-Dfile.encoding=UTF-8</argLine> <properties> <property> <name>usedefaultlisteners</name> 
				<value>false</value> </property> <property> <name>listener</name> <value> 
				注意替换,该类可以参考BeautyReporter编写 com.itrus.common.reporter.BeautyReporter </value> 
				</property> </properties> </configuration> </plugin> -->
		</plugins>
	</build>
	<properties>
		<java.version>1.8</java.version>
		<httpclient.version>4.5.6</httpclient.version>
		<okhttp.version>3.10.0</okhttp.version>
		<apphome.version>4.1.11</apphome.version>
		<jave.version>1.6.4</jave.version>
		<atom-sign-api.version>1.1.4</atom-sign-api.version>
		<springfox-swagger.version>2.9.2</springfox-swagger.version>
		<tk.mybatis.version>2.1.5</tk.mybatis.version>
		<tk.mybatis.mapper.version>4.1.5</tk.mybatis.mapper.version>
	</properties>
	<!-- 私有化服务上传jar -->
	<distributionManagement>
		<repository>
			<id>maven-itrus-releases</id>
			<name>maven-itrus-releases</name>
			<url>http://mv.6rencn.com:9191/repository/maven-itrus-releases/</url>
		</repository>
		<snapshotRepository>
			<id>maven-itrus-snapshots</id>
			<name>maven-itrus-snapshots</name>
			<url>http://mv.6rencn.com:9191/repository/maven-itrus-snapshots/</url>
		</snapshotRepository>
	</distributionManagement>
	<dependencies>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-data-redis</artifactId>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-web</artifactId>
			<exclusions>
				<!-- 切换log4j2日志读取 -->
				<exclusion>
					<groupId>org.springframework.boot</groupId>
					<artifactId>spring-boot-starter-logging</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.squareup.okhttp3</groupId>
			<artifactId>okhttp</artifactId>
			<version>${okhttp.version}</version>
		</dependency>
		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<optional>true</optional>
		</dependency>

		<!--通用mapper -->
		<dependency>
			<groupId>tk.mybatis</groupId>
			<artifactId>mapper-spring-boot-starter</artifactId>
			<!-- <scope>compile</scope> -->
			<version>${tk.mybatis.version}</version>
		</dependency>
		<dependency>
			<groupId>tk.mybatis</groupId>
			<artifactId>mapper</artifactId>
			<scope>compile</scope>
			<version>${tk.mybatis.mapper.version}</version>
		</dependency>
		<!--接口文档生成工具 -->
		<dependency>
			<groupId>io.springfox</groupId>
			<artifactId>springfox-swagger2</artifactId>
			<scope>compile</scope>
			<version>${springfox-swagger.version}</version>
		</dependency>
		<dependency>
			<groupId>io.springfox</groupId>
			<artifactId>springfox-swagger-ui</artifactId>
			<scope>compile</scope>
			<version>${springfox-swagger.version}</version>
		</dependency>

		<!-- 发送邮件 -->
		<dependency>
			<groupId>com.sun.mail</groupId>
			<artifactId>javax.mail</artifactId>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.apache.commons</groupId>
			<artifactId>commons-lang3</artifactId>
			<scope>compile</scope>
		</dependency>
		<!-- json -->
		<dependency>
			<groupId>org.apache.httpcomponents</groupId>
			<artifactId>httpclient</artifactId>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.apache.httpcomponents</groupId>
			<artifactId>httpmime</artifactId>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>cn.com.itrus.atom.sign</groupId>
			<artifactId>atom-sign-api</artifactId>
			<scope>compile</scope>
			<version>${atom-sign-api.version}</version>
		</dependency>
		<!-- 添加APPHOME -->
		<dependency>
			<groupId>com.itrus.newportal</groupId>
			<artifactId>apphome</artifactId>
			<scope>compile</scope>
			<version>${apphome.version}</version>
		</dependency>
		<dependency>
			<groupId>com.itrus</groupId>
			<artifactId>jave</artifactId>
			<scope>compile</scope>
			<version>${jave.version}</version>
		</dependency>
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-test</artifactId>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-configuration-processor</artifactId>
			<optional>true</optional>
		</dependency>
		<!-- testNG start -->
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-test</artifactId>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>com.vimalselvam</groupId>
			<artifactId>testng-extentsreport</artifactId>
			<version>1.3.1</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>com.aventstack</groupId>
			<artifactId>extentreports</artifactId>
			<version>3.0.6</version>
			<scope>test</scope>
		</dependency>
		<!-- testNG end -->
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.12</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi</artifactId>
			<version>3.17</version>
		</dependency>
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi-ooxml</artifactId>
			<version>3.17</version>
		</dependency>
		<dependency>
			<groupId>commons-collections</groupId>
			<artifactId>commons-collections</artifactId>
			<version>3.2.1</version>
		</dependency>
		<dependency>
			<groupId>commons-beanutils</groupId>
			<artifactId>commons-beanutils</artifactId>
			<version>1.8.3</version>
		</dependency>
	</dependencies>
</project>
